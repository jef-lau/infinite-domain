<!doctype html>

<html>
<div class="appContainer">
  <a><img src="/images/icons/mint.png" class="app" data-toggle="modal" data-target="#mint" data-backdrop="true" data-keyboard="true"></a>
  <p class="appName">Mint</p>
</div>

<!-- Modal -->
<div id="mint" class="modal fade" role="dialog"  tabindex='-1'>
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">

      <div class="modal-body" id="mintbody">
        {{>mint-updatesT}}                    
      </div>
      <div class="modal-footer">
        <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times"></i></button>
      </div>
    </div>
  </div>
</div>  
    
</html>
