<!doctype html>


<div class="appContainer">
  <a><img src="/images/icons/messenger.png" class="app" data-toggle="modal" data-target="#messenger" data-backdrop="true" data-keyboard="true"></a>
  <p class="appName">Messenger</p>
</div>

<!-- Modal -->
<div id="messenger" class="modal fade" role="dialog"  tabindex='-1'>
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">

      <div class="modal-body" id="messengerbody">
        {{>messenger-T}}                    
      </div>
      <div class="modal-footer">
        <button type="button" class="close" data-dismiss="modal"><i class="fa fa-times"></i></button>
      </div>
    </div>
  </div>
</div>  

<script src="/js/walt.js"></script>

